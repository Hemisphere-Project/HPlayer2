# Raspberry Pi 3
#

pcm.jack { 
    type hw
    card Headphones
}

ctl.jack {
    type hw
    card Headphones
}

pcm.hdmi0 { 
    type hw
    card b1
}

ctl.hdmi0 {
    type hw
    card b1
}

pcm.usb { 
    type hw
    card Device
}

ctl.usb {
    type hw
    card Device
}

# sets the default output device to the "pcm.both" device 
pcm.!default jack
ctl.!default jack
